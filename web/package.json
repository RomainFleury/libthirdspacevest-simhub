{
  "name": "libthirdspacevest-simhub",
  "version": "1.0.0",
  "description": "Developer tooling and Electron debugger workspace for libthirdspacevest-simhub",
  "private": true,
  "type": "module",
  "main": "electron/main.ts",
  "scripts": {
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 tsx electron/main.ts",
    "build": "vite build",
    "repomix": "repomix .. --config repomix.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomainFleury/libthirdspacevest-simhub.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/RomainFleury/libthirdspacevest-simhub/issues"
  },
  "homepage": "https://github.com/RomainFleury/libthirdspacevest-simhub#readme",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^32.1.2",
    "postcss": "^8.4.47",
    "repomix": "^1.9.1",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "wait-on": "^7.2.0"
  }
}
